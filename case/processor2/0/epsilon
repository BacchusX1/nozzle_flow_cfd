FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 377.336;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 377.336;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            epsilonWallFunction;
        value           nonuniform List<scalar> 0();
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 377.336;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 377.336;
    }
}


